  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve --host 0.0.0.0 --poll 1",
    "build:ditectrev:staging": "ng build --project=ditectrev",
    "build:ditectrev:production": "ng build --project=ditectrev --prod",
    "build:ditectrev:ssr:staging": "npm run build:ditectrev:staging && ng run ditectrev:server && npm run compile:ditectrev:server",
    "build:ditectrev:ssr:production": "npm run build:ditectrev:production && ng run ditectrev:server:production && npm run compile:ditectrev:server",
    "build-and-serve:ditectrev:ssr": "npm run build:ditectrev:ssr:staging && npm run serve:ditectrev:ssr",
    "build-and-serve:ditectrev:ssr:firebase": "npm run build:ditectrev:ssr:staging && npm run serve:ditectrev:ssr:firebase",
    "compile:ditectrev:server": "webpack --config webpack.server.config.js",
    "lint": "nx workspace-lint && ng lint",
    "test": "ng test || exit 0",
    "e2e": "ng e2e",
    "predeploy": "firebase use --token $FIREBASE_DEPLOY_KEY $CI_ENVIRONMENT_SLUG",
    "deploy": "firebase deploy --token $FIREBASE_DEPLOY_KEY",
    "deploy:cloud_functions": "cd functions && firebase deploy --token $FIREBASE_DEPLOY_KEY",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "serve:ditectrev:ssr": "node functions/index",
    "serve:ditectrev:ssr:firebase": "firebase serve"
  },
