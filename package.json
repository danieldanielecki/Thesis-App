    "build:ditectrev:ssr": "npm run build:ditectrev && ng run ditectrev:server:production && npm run compile:ditectrev:server",
    "build-and-serve:ditectrev:ssr": "npm run build:ditectrev:ssr && npm run serve:ditectrev:ssr",
