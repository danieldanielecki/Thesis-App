    "compile:ditectrev:server": "webpack --config webpack.config.js",
    "lint": "./node_modules/.bin/nx lint && ng lint --type-check",
    "test": "ng test || exit 0",
    "e2e": "ng e2e",
    "predeploy": "firebase use --token $FIREBASE_DEPLOY_KEY $CI_ENVIRONMENT_SLUG",
    "deploy": "firebase deploy --only hosting --token $FIREBASE_DEPLOY_KEY",
    "deploy:cloud_functions": "cd functions && firebase deploy --only functions,hosting --token $FIREBASE_DEPLOY_KEY",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
